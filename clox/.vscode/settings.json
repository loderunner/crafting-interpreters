{
  "C_Cpp.default.cStandard": "c17",
  "[makefile]": {
    "editor.insertSpaces": false,
    "editor.tabSize": 4
  },
  "[c]": {
    "files.insertFinalNewline": true
  },
  "C_Cpp.clang_format_sortIncludes": false,
  "C_Cpp.loggingLevel": "Debug",
  "files.associations": {
    "*.embeddedhtml": "html",
    "*.css": "tailwindcss",
    "CODEOWNERS": "codeowners",
    "LICENSE": "plaintext",
    "strlist.h": "c",
    "chunk.h": "c"
  }
}
