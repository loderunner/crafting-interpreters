{
  "C_Cpp.default.cStandard": "c17",
  "[makefile]": {
    "editor.insertSpaces": false,
    "editor.tabSize": 4
  },
  "[c]": {
    "files.insertFinalNewline": true
  },
  "C_Cpp.clang_format_sortIncludes": false,
  "C_Cpp.loggingLevel": "Debug",
  "files.associations": {
    "*.embeddedhtml": "html",
    "*.css": "tailwindcss",
    "CODEOWNERS": "codeowners",
    "LICENSE": "plaintext",
    "strlist.h": "c",
    "chunk.h": "c",
    "queue": "c",
    "stack": "c",
    "__hash_table": "c",
    "__split_buffer": "c",
    "array": "c",
    "bitset": "c",
    "deque": "c",
    "initializer_list": "c",
    "string": "c",
    "string_view": "c",
    "unordered_map": "c",
    "vector": "c",
    "errno.h": "c",
    "stdio.h": "c",
    "_stdio.h": "c",
    "_printf.h": "c",
    "string.h": "c",
    "_string.h": "c",
    "strings.h": "c",
    "_strings.h": "c"
  },
  "C_Cpp.codeAnalysis.clangTidy.checks.disabled": [
    "clang-analyzer-security.insecureAPI.DeprecatedOrUnsafeBufferHandling"
  ]
}
