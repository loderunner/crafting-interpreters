{
  "eslint.workingDirectories": ["jslox"],
  "C_Cpp.default.cStandard": "c17",
  "C_Cpp.clang_format_style": "file:${workspaceFolder}/clox/.clang-format",
  "[makefile]": {
    "editor.insertSpaces": false,
    "editor.tabSize": 4
  },
  "[c]": {
    "files.insertFinalNewline": true
  },
  "files.associations": {
    "*.css": "css",
    "stdlib.h": "c",
    "strlist.h": "c",
    "string.h": "c",
    "dmalloc.h": "c",
    "test_strlist.h": "c"
  },
  "C_Cpp.clang_format_sortIncludes": false
}
